{
  "name": "1Word",
  "description": "Learn a language while browsing the web",
  "version": null,
  "manifest_version": 2,
  "icons": {
    "48": "icons/icon_48.png",
    "128": "icons/icon_128.png"
  },
  "browser_action": {
    "default_title": "1Word",
    "default_popup": "popup/popup.html"
  },
  "background": {
    "scripts": ["background.js"]
  },
  "permissions": ["unlimitedStorage", "storage", "tabs", "activeTab"]
}
